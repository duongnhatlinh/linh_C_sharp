@model List<File>
@if (Model != null)
{
    foreach (var item in Model)
    {
        <tr role="row" class="odd">
            <td> @item.FileId</td>
            <td>@item.FileName</td>
            <td>@item.TypeFile.TypeFileName</td>
            <td>@item.DateUploaded </td>
            <td class="text-right">
                <a class="btn btn-primary btn-tone m-r-5" asp-area="Admin" asp-controller="AdminPermissViewFileAll" asp-action="PermissViewFile" asp-route-fileid="@item.FileId">Chọn</a>

            </td>
        </tr>
    }
}